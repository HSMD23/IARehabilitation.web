@model IEnumerable<IARehabilitation.web.Data.Entities.Consulte>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_Sportman)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_Profesional)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id_Sportman)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id_Profesional)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_Consulte">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_Consulte">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_Consulte">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
